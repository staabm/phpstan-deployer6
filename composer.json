{
  "name": "staabm/phpstan-deployer6",
  "type": "phpstan-extension",
  "license": "MIT",
  "require": {
    "php": "^7 || ^8"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "extra": {
    "phpstan": {
      "includes": [
        "config/deployer6.neon"
      ]
    }
  }
}